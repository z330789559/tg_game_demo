{
  "compilerOptions": {
    "lib": [ "es2015", "es2017", "dom" ],
    "target": "ES2015",
    "module": "CommonJS",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "temp/vscode-dist",
    "forceConsistentCasingInFileNames": true

  },
  "include": [
    "assets/**/*.ts",
  ],
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ]
}